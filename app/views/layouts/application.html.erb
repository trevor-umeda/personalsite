<!DOCTYPE html>
<html>
<head>
  <title>Site of Trevor Umeda</title>
  <%= stylesheet_link_tag    "application" %>
  <%= javascript_include_tag 'application' %>

  <%= csrf_meta_tags %>
</head>
<body>
<div id="wrap">
<div id="header" class="shadow">
  <%= render "layouts/header" %>

</div>
<div id="body-wrapper">
<div id="int-site-container">
  <div id ="left_column">
    <div id="blog_tags">
    <% if @tags %>
        Tags: </br>
    <%= render @tags  %>
      <% end %>
    </div>

  </div>
  <div id="right_column">
    <div id="additional_contact">
      additional contact
      <%= link_to "Facebook","http://www.facebook.com/trevor.umeda" %>
    </div>

  </div>
  <div id="int-site">
    <%= yield %>
    <%= debug(params) if Rails.env.development? %>

  </div>

</div>
</div>

<div id="footer">

</div>
</div>
</body>
</html>
